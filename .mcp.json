{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp",
        "--cdp-endpoint",
        "http://127.0.0.1:9225"
      ],
      "env": {
        "NO_PROXY": "127.0.0.1,localhost",
        "no_proxy": "127.0.0.1,localhost"
      }
    },
    "figma-desktop": {
      "type": "http",
      "url": "http://localhost:3845/mcp"
    }
  }
}